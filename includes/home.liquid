<!doctype html>
<html lang="{{ metadata.locale }}" prefix="og: https://ogp.me/ns#">
<!--! Hello you curious user, if it helps you can see the full code over on GitHub: {{ metadata.repo }} -->

<head>
  <!-- Google Tag Manager -->
  <script>(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
  new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
  j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
  'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
  })(window,document,'script','dataLayer','GTM-KM52M2F');</script>
  <!-- End Google Tag Manager -->
  {% include "partials/head.liquid" %}
</head>

<body>
  
  <!-- Google Tag Manager (noscript) -->
  <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-KM52M2F"
  height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
  <!-- End Google Tag Manager (noscript) -->
  
  <a href="#skip" class="skip-link">Skip to main content</a>
  <div class="hfeed h-feed layout" role="document">
    {% include "partials/header.liquid" %}
    <main class="container main" id="#skip" tabindex="-1" data-input-path="{{ page.filePathStem }}">
      <div class="page">
        <header class="page--header">
          <h1 class="page--title p-name">{{ title }}</h1>
          <div class="tagline">
            {% if subhead %}
            <p>{{ subhead }}</p>
            {% endif %}
          </div><!-- .tagline -->
        </header><!-- .page--header -->
        {% include "partials/hcard.liquid" %}
        <div class="home--page">
          {{ content }}
        </div>
      </div><!-- .page--content -->
    </main>
    {% include "partials/footer.liquid" %}
  </div><!-- .container -->
</body>

</html>
