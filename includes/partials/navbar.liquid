<figure>
  <img src="/assets/images/logo_dark.svg" alt="Site logo" width="64" height="43" eleventy:ignore />
</figure>
<nav class="nav" aria-label="primary">
  <menu class="nav-bar">
  {% for item in metadata.navigation_primary %}
    <li class="nav-item {% if item.class %}{{ item.class }}{% endif %}">
      <a class="nav-link" {% if page.url==item.url %} aria-current="page" {% endif %} href="{{ item.url }}" title="{% if item.title %}{{ item.title }}{% endif %}">
        {{ item.label }}
      </a>
    </li>
  {% endfor %}
  </menu>
</nav>
