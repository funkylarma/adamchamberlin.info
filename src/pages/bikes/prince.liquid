{% assign serviceHistory = collections.serviceList | servicesByBikeName: bike %}
{% if serviceHistory.length > 0 %}
<ul>
  {% for service in serviceHistory %}
    <li>
      <a class="post-link u-url" rel="bookmark" href="{{ service.url }}" title="Continue to view {{ service.data.title | escape }}">
        {{ service.data.title | escape | strip_html | truncate: 40 }}
      </a>
      - {{ service.data.date | dateReadable }}
    </li>
  {% endfor %}
</ul>
{% else %}
  <p>This bike has not been serviced yet.</p>
{% endif %}